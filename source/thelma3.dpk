  standardfuncs in 'standardfuncs.pas',
  optim_routines in 'optim_routines.pas',
  vagmatrix in 'vagmatrix.pas',
  groundwater in 'groundwater.pas',
  tmpfiles in 'tmpfiles.pas',
  ts in 'ts.pas',
  hydromodel in 'hydromodel.pas',
  interpol in 'interpol.pas',
  montecarlo in 'montecarlo.pas',
  statprocesses in 'statprocesses.pas',
  tsdblmass in 'tsdblmass.pas',
  tsevap in 'tsevap.pas',
  tshydraulics in 'tshydraulics.pas',
  tsidf in 'tsidf.pas',
  tsprocess in 'tsprocess.pas',
  sparsolv in 'sparsolv.pas',
  isets in 'isets.pas',
  tsarea in 'tsarea.pas',
  itzones in 'itzones.pas',
  climacgr in 'climacgr.pas';

end.
