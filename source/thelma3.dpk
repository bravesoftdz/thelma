  designide,
  xmlrtl,
  vclactnband,
  vclx;

contains
  genutils in 'genutils.pas',
  dkmath in 'dkmath.pas',
  stat2 in 'stat2.pas',
  dates in 'dates.pas',
  istrutils in 'istrutils.pas',
  prob in 'prob.pas',
  matrix in 'matrix.pas',
  rnd in 'rnd.pas',
  geotrans in 'geotrans.pas',
  ifile in 'ifile.pas',
  calculus in 'calculus.pas',
